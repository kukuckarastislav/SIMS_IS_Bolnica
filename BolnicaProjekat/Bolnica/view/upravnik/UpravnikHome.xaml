<Window x:Class="Bolnica.view.upravnik.UpravnikHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bolnica.view.upravnik"
        mc:Ignorable="d"
        Title="UpravnikHome" Height="720" Width="1280">
    <Grid>
        <DockPanel>
            <Grid DockPanel.Dock="Left" Background="Aqua" Width="Auto">
                <StackPanel Orientation="Vertical">
                    <Button Margin="6" Click="Prostorije_Click">Ras sa prostorijama</Button>
                    <Button Margin="6">ToDO</Button>
                    <Button Margin="6">ToDO</Button>
                    <Button Margin="6">ToDO</Button>
                    <Button Margin="6">ToDO</Button>
                </StackPanel>
            </Grid>
            <DockPanel DockPanel.Dock="Right">
                <StackPanel DockPanel.Dock="Top" Height="60" Background="DeepPink"></StackPanel>
                <Frame DockPanel.Dock="Bottom" x:Name="RadnaPovrsina" NavigationUIVisibility="Hidden"></Frame>
            </DockPanel>
          
        </DockPanel>
    </Grid>
</Window>
